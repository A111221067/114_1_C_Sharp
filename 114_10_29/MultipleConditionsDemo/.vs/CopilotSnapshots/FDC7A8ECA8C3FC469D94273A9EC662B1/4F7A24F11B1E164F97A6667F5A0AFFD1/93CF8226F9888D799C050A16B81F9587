        private void btnJudge_Click(object sender, EventArgs e)
        {
            try
            {
                int score = int.Parse(txtScore.Text);
                string grade;
                if (score >= 60)
                {
                    if (score <= 70)
                    {
                        if (score >=80)
                        {
                            if (score >= 90)
                            {
                                grade = "A";
                            }
                            else
                            {
                                grade = "B";
                            }
                        }
                        else
                        {
                            grade = "C";
                        }
                    }
                    else
                    {
                        grade = "D";
                    }
                }
                else
                {
                    grade = "E";
                }
                {
                    
                }
            }
        }
    }
